{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.2"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        },
        {
            "src": "/user(.*)",
            "dest": "/api/user.php"
        },
        {
            "src": "/controller(.*)",
            "dest": "/api/controller.php"
        }
    ]
}